        </dependency>


        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-thymeleaf</artifactId>
            <version>3.1.0</version> <!-- Bảo đảm >= 3.1 -->
        </dependency>

        <!-- Runtime, com.sun.xml.bind module -->
